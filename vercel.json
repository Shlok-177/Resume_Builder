{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "./server.js"
      },
      {
        "src": "/",
        "dest": "client/build/index.html"
      },
      {
        "src": "/",
        "dest": "client/build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }